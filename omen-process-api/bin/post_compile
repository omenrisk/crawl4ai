#!/bin/sh
echo "Post compile script but its clean..."
apt-get update && apt-get install -y libnss3 libatk1.0-0 libatk-bridge2.0-0 libxkbcommon0 libatspi2.0-0 libxcomposite1 libxdamage1 libxfixes3 libxrandr2 libgbm1 libasound2
echo "Installing Playwright dependencies..."
playwright install-deps
# echo "Installing Playwright browser..."
# export PLAYWRIGHT_BROWSERS_PATH=/app/.playwright-browsers
python -m playwright install chromium
